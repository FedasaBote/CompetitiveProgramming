def insertionSort1(n, arr):
    # Write your code here
    v=arr[n-1]
    done=0
    for i in range(n-1,0,-1):
            if(arr[i-1]<=v):
                    arr[i]=v
                    done=1
                    break
            else:
                    arr[i]=arr[i-1]
            print (" ".join(map(str,arr)))
    if (i==1 and done==0):
            arr[i-1]=v
    print (" ".join(map(str,arr)))
