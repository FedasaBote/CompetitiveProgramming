class Solution(object):
    def dailyTemperatures(self, temperatures):
        """
        :type temperatures: List[int]
        :rtype: List[int]
        """
        stack=[]
        ans = [0 for temp in temperatures]
        count=0
        for i in range(len(temperatures)):
            while stack and temperatures[stack[-1]]<temperatures[i]:
                index=stack.pop()
                ans[index]=i-index

            stack.append(i)

        return ans
